{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "CORE_CM7",
                "STM32H750xx",
                "STM32H750IB",
                "ARM_MATH_CM7",
                "flash_layout",
                "HSE_VALUE=16000000",
                "USE_HAL_DRIVER",
                "USE_FULL_LL_DRIVER"
            ],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/local/bin/arm-none-eabi-g++",
            "intelliSenseMode": "macos-gcc-arm",
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "compilerArgs": [
                "-fno-exceptions",
                "-fno-rtti",
                "-std=gnu++14",
                "-std=gnu11",
                "-Wall",
                "-Wno-attributes",
                "-Wno-strict-aliasing",
                "-Wno-maybe-uninitialized",
                "-mcpu=cortex-m7",
                "-mfpu=fpv5-d16",
                "-mfloat-abi=hard",
                "-mthumb"
            ]
        }
    ],
    "version": 4
}